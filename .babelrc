{
    "plugins": ["transform-decorators-legacy"],
    "presets": ["react", ["env", {"modules": false}], "stage-0"],
    "env": {
        "development": {
            "plugins": [
                [
                    "react-transform",
                    {
                        "transforms": [
                            {
                                "transform": "react-transform-hmr",
                                "imports": ["react"],
                                "locals": ["module"]
                            }
                        ]
                    }
                ]
            ]
        }
    }
}
